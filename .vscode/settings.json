{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.options": {
    "extensions": [".js", ".jsx", ".ts", ".tsx"]
  },
  "tailwindCSS.experimental.classRegex": [
    ["customClass\\w*\\s*=\\s*'(.*)'"],
    ["customClass\\w*\\s*=\\s*\"(.*)\""],
    ["customClass\\w*\\s*=\\s*`(.*)`"],
  ]
}
